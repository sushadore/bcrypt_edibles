<h1> Sign up</h1>
<div class="row">
  <div class="col-xs-6">
    <%= bootstrap_form_for @user do |f| %>
      <%= f.text_field :email %>
      <%= f.text_field :username %>
      <%= f.password_field :password %>
      <%= f.password_field :password_confirmation %>
      <%= f.submit "Sign Up" %>
    <% end %>
  </div>
</div>
